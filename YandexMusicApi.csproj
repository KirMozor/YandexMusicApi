<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <PackageId>YandexMusicApi</PackageId>
        <version>1.1.0</version>
		<Authors>KirMozor</Authors>
        <Description>Open-Source wrapper over the Yandex Music Api</Description>
        <PackageIconUrl>https://download.cdn.yandex.net/from/yandex.ru/support/ru/music/files/icon_main.png</PackageIconUrl>
		<RepositoryUrl>https://github.com/KirMozor/YandexMusicApi</RepositoryUrl>
        <PackageProjectUrl>https://github.com/KirMozor/YandexMusicApi</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/KirMozor/YandexMusicApi/blob/main/LICENSE</PackageLicenseUrl>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <TargetFramework>net6.0</TargetFramework>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    </ItemGroup>

</Project>
